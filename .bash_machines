### Remote Machines

# csh
alias fury="ssh askedrlc@fury.csh.rit.edu"
alias scorn="ssh askedrlc@scorn.csh.rit.edu"

# me
alias odin="ssh askedrelic@asktherelic.com"
alias frig="ssh -p 30000 askedrelic@localhost -o 'ProxyCommand=ssh -q askedrelic@asktherelic.com exec nc %h %p'"

alias asktherelic=odin
alias asktherelic-mysql="ssh -L 3306:localhost:3306 askedrelic@asktherelic.com"
alias asktherelic-forward="ssh -nNT -R 1100:localhost:22 askedrelic@asktherelic.com &"
